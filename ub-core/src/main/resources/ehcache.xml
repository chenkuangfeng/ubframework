<?xml version="1.0" encoding="gbk"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd">
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="30" timeToLiveSeconds="30" overflowToDisk="false" />
	<!-- 配置自定义缓存 maxElementsInMemory：缓存中允许创建的最大对象数 eternal：缓存中对象是否为永久的，如果是， 
		超时设置将被忽略，对象从不过期。 timeToIdleSeconds：缓存数据的钝化时间，也就是在一个元素消亡之前， 两次访问时间的最大时间间隔值， 
		这只能在元素不是永久驻留时有效， 如果该值是 0 就意味着元素可以停顿无穷长的时间。 timeToLiveSeconds：缓存数据的生存时间， 也就是一个元素从构建到消亡的最大时间间隔值， 
		这只能在元素不是永久驻留时有效，如果该值是0就意味着元素可以停顿无穷长的时间。 overflowToDisk：内存不足时，是否启用磁盘缓存。 memoryStoreEvictionPolicy：缓存满了之后的淘汰算法。 -->
	<cache name="SUBJECT" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	
	<cache name="LOOKUP" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="SESSION" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="PERMISSION" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="DIMENSION" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="BIOMETA" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="FDMMETA" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
		<cache name="FORMMETA" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
		
	<cache name="REPORT" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="TASK" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="WXACCOUNT" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="ACCESSTOKEN" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
		<cache name="ENDPOINT" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
		<cache name="APPKEY" maxElementsInMemory="100000" eternal="true"
		overflowToDisk="true" timeToIdleSeconds="3600" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU" />
		

</ehcache>